{
  "name": "kirby-markdown-field",
  "version": "0.1.0",
  "description": "Enhanced markdown editor for Kirby 3",
  "main": "index.js",
  "author": "Kirby Community",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "watchify -vd -p browserify-hmr -e src/main.js -o index.js",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o index.css ] -p bundle-collapser/plugin -e src/main.js | uglifyjs -c warnings=false -m > index.js && gulp"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "browserify-hmr": "^0.3.6",
    "bundle-collapser": "^1.3.0",
    "codemirror": "^5.42.0",
    "cross-env": "^5.2.0",
    "envify": "^4.1.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "node-sass": "^4.9.4",
    "uglify-js": "^3.4.7",
    "vue": "^2.5.17",
    "vueify": "^9.4.1",
    "watchify": "^3.11.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
